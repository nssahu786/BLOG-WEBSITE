<%- include('../header.ejs') %>


<section class="login-wrap mtb-40">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-8">
                <div class="login-box" style="width: 800px;">
                    <h1 class="text-success">VIEW DATA</h1>
                    <table class="table table-bordered">
                        <tr>
                            <th>SNo</th>
                            <th>id</th>
                            <th>Name</th>
                            <th>Email</th>  
                        </tr>
                        <% var i=1 %>
                            <!-- console.log(item) -->
                            <tr>
                                <td><%= i++ %></td>
                                <td><%= data._id %></td>
                                <td><%= data.name %></td>
                                <td><%= data.email %></td>
                            </tr>
                    </table>
                </div>         
            </div>
        </div>
    </div>
</section>


<%- include('../footer.ejs') %>